<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Ofl.Twitch.Abstractions implementation registrations in Autofac.</Description>
    <TargetFramework>netstandard1.3</TargetFramework>
    <AssemblyName>Ofl.Twitch.DependencyInjection.Autofac</AssemblyName>
    <PackageId>Ofl.Twitch.DependencyInjection.Autofac</PackageId>
    <PackageProjectUrl>https://github.com/OneFrameLink/Ofl.Twitch.DependencyInjection.Autofac</PackageProjectUrl>
    <RepositoryUrl>https://github.com/OneFrameLink/Ofl.Twitch.DependencyInjection.Autofac.git</RepositoryUrl>
	<Version>2.0.0</Version>
	<PackageTags>twitch twitch-api twitch-api-v5 dependency-injection autofac</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Autofac.Extensions.DependencyInjection" Version="4.1.0" />
    <PackageReference Include="Ofl.Twitch" Version="2.0.8" />
    <PackageReference Include="Ofl.Twitch.Abstractions" Version="1.0.2" />
  </ItemGroup>

</Project>
